---
import Coordiante from "./Coordinate.astro";

export interface Props {
  x: string;
  y: string;
  [key: string]: string;
}

const { x, y, ...props } = Astro.props as Props;
---

<g class="element">
  <rect x={x} y={y} {...props}></rect>
  <g class="coordinates">
    <Coordiante x={x} y={y} />
  </g>
</g>
